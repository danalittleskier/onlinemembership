<decorators defaultdir="/decorators">
	<excludes>
		<pattern>/40*.jsp</pattern>
		<pattern>/*ajax=true*</pattern>
		<pattern>/scripts/greybox/*</pattern>
		<pattern>/scripts/dojo/*</pattern>
		<pattern>/struts/dojo/*</pattern>
		<pattern>/resources/*</pattern>
	</excludes>
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="basicDecorator" page="basicDecorator.jsp">
		<pattern>/certificate.html</pattern>
		<pattern>/sportInfo.html</pattern>
		<pattern>/backgroundScreeningPopup.html</pattern>
	</decorator>
</decorators>
